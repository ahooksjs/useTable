{
  "name": "root",
  "private": true,
  "scripts": {
    "start": "dumi dev",
    "dev": "dumi dev",
    "add:pkg": "node ./scripts/add",
    "cdn": "node ./scripts/cdn",
    "surge": "node ./scripts/surge",
    "docs:build": "node ./scripts/version && dumi build && npm run cdn && npm run surge",
    "docs:pub": "surge ./dist --domain usetable-ahooks.js.org; surge ./dist --domain usetable-ahooks.surge.sh",
    "docs:deploy": "npm run docs:build && npm run docs:pub",
    "doc:deploy:useVersion": "dumi build && npm run surge && npm run docs:pub",
    "build": "father-build && lerna run build:tsc",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "add": "lerna add",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "jest",
    "preversion": "npm run build && npm run test",
    "version:alpha": "lerna version prerelease --preid alpha --force-publish=* --no-git-tag-version",
    "version:preminor:alpha": "lerna version preminor --preid alpha --force-publish=* --no-git-tag-version",
    "release:force": "lerna publish from-package --yes",
    "release": "lerna publish"
  },
  "devDependencies": {
    "@alifd/next": "^1.20.18",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@formily/antd": "^1.3.8",
    "@formily/antd-components": "^1.3.8",
    "@formily/next": "^1.3.8",
    "@formily/next-components": "^1.3.8",
    "@formily/react-schema-renderer": "^1.2.9",
    "@testing-library/react": "^10.4.8",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/jest": "^26.0.5",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "ansi-styles": "^4.2.1",
    "antd": "^4.9.4",
    "babel-jest": "^26.1.0",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "camelcase": "^6.0.0",
    "dumi": "^1.0.33",
    "eslint": "^7.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "execa": "^4.0.3",
    "father-build": "^1.18.2",
    "fs-extra": "^9.0.1",
    "jest": "^26.1.0",
    "lerna": "^3.22.1",
    "meow": "^7.0.1",
    "moment": "^2.27.0",
    "react-eva": "^1.1.10",
    "react-test-renderer": "^16.13.1",
    "rxjs": "^6.6.2",
    "styled-components": "^5.1.1",
    "surge": "^0.21.6",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7",
    "use-template": "^0.3.4"
  }
}
